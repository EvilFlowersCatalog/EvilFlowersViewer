{
  "name": "@evilflowers/evilflowersviewer",
  "version": "0.4.10",
  "type": "module",
  "main": "./dist/evilFlowersViewer.umd.js",
  "module": "./dist/evilFlowersViewer.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/evilFlowersViewer.es.js",
      "require": "./dist/evilFlowersViewer.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:css",
    "build:staging": "vite build --mode staging && npm run build:css",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "build:css": "tailwindcss -m -i ./src/index.css -o ./dist/styles.css"
  },
  "dependencies": {
    "citation-js": "^0.6.8",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@angular/core": "~12.1.4",
    "@citation-js/plugin-bibtex": "^0.6.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "i18next": "^22.4.14",
    "pdfjs-dist": "^2.16.105",
    "postcss": "^8.4.21",
    "react-i18next": "^12.2.0",
    "rollup": "^3.20.1",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "files": [
    "dist"
  ]
}
